# Advanced Minecraft Plugin Environment Configuration
# Copy this file to .env and modify the values as needed for your server setup.

# MongoDB Configuration
MONGO_URI=mongodb://localhost:27017
MONGO_DATABASE=minecraft_plugin

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# gRPC Configuration
GRPC_ENABLED=true
GRPC_PORT=9090
GRPC_SECRET_KEY=your_secure_secret_key_change_this_immediately

# Optional: Override plugin settings via environment
# PLUGIN_DEBUG_MODE=false
# PLUGIN_AUTO_SAVE_ENABLED=true
# PLUGIN_AUTO_SAVE_INTERVAL=300 